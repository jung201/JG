package AA;

import java.util.Arrays;

public class sample_3 {

	public static void main(String[] args) {
		
	int[] lotto = new int[6];		
		
		for (int i = 0; i < 6 ; i++) {
			lotto[i]= (int) (Math.random() * 9) + 1;
			
			for(int j = 0; j < i; j++) {
				if(lotto[i] == lotto[j]) {
					i--;
					break;
				}
			}
		} 
		System.out.println(lotto);
	}
}