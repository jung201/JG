package AA;

import java.util.Iterator;
import java.util.Random;
import java.util.Scanner;

public class sample_3 {

	public static void main(String[] args) {
	
		int A = (int) (Math.random() * 9) + 1;
		for (int i = 0; i < 6 ; i++) {
			for (int j = 0; j <  i; j++) {
				System.out.println(A);
			}
		}
		
	}
}